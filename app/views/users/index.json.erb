<% if @users.present? %>
	[
		<% @users.each_with_index do |user,index| %>
			{
                "id" : <%= user.id %>,
				"username" : "<%= user.username %>",
				"display_name" : "<%= user.display_name %>",
				"status" : "<%= user.status %>",
				"media_id" : <%= user.media_id.present? ? user.media_id : -1 %>
			}
            <% if index != @users.length - 1 %>
            ,
            <% end %>
		<% end %>
	]
<% end %>