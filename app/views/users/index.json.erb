<% if @users.present? %>
	[
		<% @users.each_with_index do |index,user| %>
			{
                "id" : <%= user.id %>,
				"username" : "<%= user.username %>",
				"display_name" : "<%= user.display_name %>",
				"status" : "<%= user.status %>",
				"media_id" : <%= user.media_id.present? ? user.media_id : -1 %>
			}
            <% p ',' unless index == @users.length %>
		<% end %>
	]
<% end %>